    "@radix-ui/react-tooltip": "^1.2.8",
    "@serwist/next": "^9.5.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.17",
    "@trpc/client": "^11.0.0",
