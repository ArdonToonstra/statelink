    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@types/web-push": "^3.6.3",
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8",
    "eslint-config-next": "^16.1.4",
    "postcss": "^8.5.6",
    "sass": "^1.97.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
