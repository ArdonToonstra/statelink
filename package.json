    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.26.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "next": "^16.1.4",
